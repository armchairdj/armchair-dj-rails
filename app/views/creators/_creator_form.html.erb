<%= simple_form_for(creator, html: { class: "crud"}) do |f| %>
  <%= f.error_notification %>

  <%= content_tag(:div, class: "form-inputs") do %>
    <%= render partial: 'creators/creator_fields', locals: { f: f } %>
  <% end %>

  <%= content_tag(:div, class: "button-group") do %>
    <%= f.button :submit %>
  <% end %>
<% end %>
