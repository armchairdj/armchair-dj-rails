<%
  @title = "Creators"
%>

<%= render "admin/shared/header", action: :index,
  prehead:  @collection.display_count,
  headline: @title
%>

<%= render "admin/shared/index",
  collection: @collection,
  records: @creators,
  columns: [
    { name: "ID",            method: :id              },
    { name: "Name",          method: :name            },
    { name: "Creator Type",  method: :membership_type },
    { name: "Identity Type", method: :identity_type   },
  ]
%>
