<%
  @headline = @title = "Admin Genres"
%>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_genres.each do |admin_genre| %>
      <tr>
        <td><%= link_to "Show", admin_genre %></td>
        <td><%= link_to "Edit", edit_admin_genre_path(admin_genre) %></td>
        <td><%= link_to "Destroy", admin_genre, method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New Admin Genre", new_admin_genre_path %>
