<% return if creators.empty? %>

<% headline = "Creators" if local_assigns[:headline].nil? %>

<%= content_tag(:section, class: "admin-section") do %>
  <%= content_tag(:header) do %>
    <%= content_tag(:h4, headline) %>
  <% end %>

  <%= content_tag(:ul, class: "square") do %>
    <%= render partial: "admin/shared/creator", collection: creators %>
  <% end %>
<% end %>
