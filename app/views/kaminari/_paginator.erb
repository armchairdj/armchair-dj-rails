<%= paginator.render do %>
  <nav class="pagination">
    <ul>
      <%= prev_page_tag %>
      <li>
        <ul>
          <% each_page do |page| %>
            <% if page.left_outer? || page.right_outer? || page.inside_window? %>
              <%= page_tag page %>
            <% elsif !page.was_truncated? %>
              <%= gap_tag %>
            <% end %>
          <% end %>
        </ul>
      </li>
      <%= next_page_tag %>
    </ul>
  </nav>
<%  end %>